<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
    <definition name="defaultTemplate" template="/WEB-INF/template/_template.jsp">
        <put-attribute name="header" value="/WEB-INF/template/header.jsp" cascade="true"/>
        <put-attribute name="navigation_bar" value="/WEB-INF/template/navigation_bar.jsp" cascade="true"/>
        <put-attribute name="content" expression="${content}"/>
        <put-attribute name="footer" value="/WEB-INF/template/footer.jsp" cascade="true"/>
    </definition>
</tiles-definitions>